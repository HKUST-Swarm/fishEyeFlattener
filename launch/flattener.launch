<launch>
    <node pkg="fisheye_flattener" type="fisheye_flattener_node" name="flattener" output="screen">
        <param name="cam_file" type ="string" value="/home/acw/Desktop/calib_images/CM3_fish_camera_calib.yaml"/>
        <param name="inputTopic" type ="string" value="/img"/>
        <param name="outputTopicPrefix" type="string" value="flatImage"/>
        <!-- rotationVector: addition rotational offset for remapping  -->
        <param name="rotationVectorX" type="double" value="1"/> 
        <param name="rotationVectorY" type="double" value="1"/> 
        <param name="rotationVectorZ" type="double" value="1"/> 
        <!-- FOV is in degree -->
        <param name="fov" type="double" value="270"/> 
        <!-- Image width for center reprojection -->
        <param name="imgWidth" type="int" value="600"/> 
    </node>
</launch>

